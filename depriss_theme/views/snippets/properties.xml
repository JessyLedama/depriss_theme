<odoo>
    
    <template id="depriss_properties" inherit_id="website.snippets" name="Properties">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="depriss_theme.properties"/>
        </xpath>
    </template>
