<odoo>
    <data>

        <template id="assets_backend" name="depriss_theme assets" inherit_id="web.assets_backend">

            <xpath expr="//link[last()]" position="after">

                <link rel="stylesheet" type="text/scss"
                    href="/depriss_theme/static/src/css/bootstrap-select.min.css"/>
                <link rel="stylesheet" type="text/scss"
                    href="/depriss_theme/static/src/css/bootstrap.min.css"/>
                <link rel="stylesheet" type="text/scss"
                    href="/depriss_theme/static/src/css/styles.css"/>

            </xpath>


            <xpath expr="." position="inside">
                <script type="text/javascript" src="/depriss_theme/static/src/js/snippets.js"></script>
            </xpath>
        </template>

    </data>
</odoo>
